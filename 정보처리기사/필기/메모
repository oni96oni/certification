    11.
        SELECT 부서, SUM(급여)
          FROM 급여
         GROUP BY 부서
        HAVING SUM(급여) >= 6000

    12.
        SELECT 이름, 과목, 학점
          FROM 성적
         ORDER BY 학점 DESC, 이름 

    13.
        SELECT 책번호, 책명, 가격
          FROM 도서 A
          JOIN 도서가격 B
            ON A.책번호 = B.책번호
    
    14.
        SELECT 책번호, 책명, 가격
          FROM 도서 A
          FULL OUTER JOIN 도서가격 B
            ON A.책번호 = B.책번호

    15.
        SELECT MAX(가격)
          FROM 도서가격
         WHERE 책번호 IN (
            SELECT 책번호
              FROM 도서
             WHERE 책명 = '자료구조'
         )
    
    16.
        INSERT INTO 학생 ('3000', '장길산', '3', '수학')
    
